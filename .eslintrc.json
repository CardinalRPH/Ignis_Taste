{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-tabs":0,
        "no-plusplus":0,
        "eqeqeq": "off",
        "no-param-reassign": ["error", { "props": false }],
        "camelcase": ["error", {"allow": ["get_all_data", "get_detail_data", "post_review", "object_builder", "Like_Button"]}],
        "no-underscore-dangle": 0,
        "no-use-before-define": ["error", { "variables": false }],
        "padded-blocks": ["error", "never"],
        "no-console": "off",
        "no-shadow": "off"
    }
}
